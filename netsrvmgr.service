[Unit]
Description= Network Manager Services (wifi, moca and bluetooth) Daemon
After= iarmbusd.service
Requires=iarmbusd.service

[Service]
ExecStart=/bin/sh -c 'source /lib/rdk/opt-override.sh;/usr/bin/netsrvmgr --debugconfig $DEBUGINIFILE --configFilePath $NETSRVMGRINIFILE'
ExecStop=/bin/kill -15 $MAINPID
Restart=always

[Install]
WantedBy=multi-user.target
